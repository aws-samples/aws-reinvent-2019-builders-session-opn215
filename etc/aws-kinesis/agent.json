{
  "cloudwatch.emitMetrics": true,
  "kinesis.endpoint": "https://kinesisanalytics.eu-west-1.amazonaws.com",
  "firehose.endpoint": "https://firehose.eu-central-1.amazonaws.com",  
  "flows": [
    {
      "filePattern": "/var/log/snort/alert.csv*",
      "deliveryStream": "aws-snort-demo-SnortAlertStream"
    },
    {
      "filePattern": "/var/log/snort/tcp_dump.log*",
      "deliveryStream": "aws-snort-demo-SnortPacketStream"
    }
  ]
}
